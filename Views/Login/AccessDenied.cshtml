
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
   
    <meta name="viewport" content="width=device-width, initial-scale=1" />
   <title>Sayfa Bulunamadı</title>
    <link rel="icon"
          type="image/png"
          sizes="x16"
          href="~/Admin/assets/images/favicon.png" />

    <link href="~/Admin/dist/css/style.min.css" rel="stylesheet" />

</head>

<body>
    <div class="main-wrapper">

        <div class="preloader">
            <div class="lds-ripple">
                <div class="lds-pos"></div>
                <div class="lds-pos"></div>
            </div>
        </div>

        <div class="error-box">
            <div class="error-body text-center">
                <h1 class="error-title text-danger">403</h1>
                <h3 class="text-uppercase error-subtitle">SAYFA BULUNAMADI !</h3>
                <p class="text-muted mt-4 mb-4">
                    EVİNİN YOLUNU BULMAYA ÇALIŞIYOR GİBİ GÖRÜNÜYORSUNUZ.
                </p>
                <a href="/HomePage/Index/"
                   class="
              btn btn-danger btn-rounded
              waves-effect waves-light
              mb-5
              text-white
            ">Ana Sayfa</a>
            </div>
        </div>

    </div>

    <script src="~/Admin/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/Admin/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script>$(".preloader").fadeOut();</script>
</body>
</html>
